{
  "version": "0.2",
  "gitignoreRoot": ".",
  "useGitignore": true,
  "dictionaries": [
    "css",
    "html",
    "fonts",
    "typescript",
    "softwareTerms",
    "companies",
    "lorem-ipsum",
    "project-words"
  ],
  "dictionaryDefinitions": [
    {
      "name": "project-words",
      "path": "./library/project-words.txt",
      "noSuggest": true
    }
  ],
  "ignorePaths": [
    "CHANGELOG.md",
    "package.json",
    "yarn.lock",
    "pnpm-lock.yaml",
    "project-words.txt",
    "__snapshots__",
    "*.xyz",
    "*.docx",
    "*.gitignore",
    "versioned_docs",
    "*.min.*",
    "jest/vendor"
  ],
  "ignoreRegExpList": ["Email", "Urls", "#[\\w-]*"]
}
