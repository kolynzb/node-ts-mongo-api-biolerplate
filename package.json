{
  "name": "node-ts-mongo-api",
  "version": "1.0.0",
  "description": "Node js typescript mongo db API setup",
  "main": "src/server.ts",
  "repository": "https://github.com/kolynzb/nest-API.git",
  "scripts": {
    "dev": "nodemon",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/server.js",
    "build": "npm run clean && swc ./src -d dist && node ./scripts/copyDirToBuild.mjs",
    "start": "node -r module-alias/register dist/server.js",
    "start:dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "start:prod": "NODE_ENV=production node -r module-alias/register Â dist/server.js",
    "clean": "rimraf dist",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "lint:dict": "node scripts/wordSort.mjs",
    "pretty": "prettier --write 'src/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "debug": "ndb ./src/server.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [],
  "author": "kolynzb <collinsbenda360@gmail.com> (https://atuhaire.com)",
  "license": "MIT",
  "_moduleAliases": {
    "@controllers": "dist/controllers",
    "@services": "dist/services",
    "@middleware": "dist/middleware",
    "@config": "dist/config",
    "@routes": "dist/routes",
    "@utils": "dist/utils",
    "@schemas": "dist/schemas",
    "@interfaces": "dist/interfaces",
    "@types": "dist/types"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.68",
    "@tsconfig/node18": "^1.0.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/convict": "^6.1.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.2",
    "@types/html-to-text": "^9.0.1",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.0",
    "@types/nodemailer": "^6.4.8",
    "@types/pug": "^2.0.6",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^11.1.1",
    "jest": "^29.6.1",
    "module-alias": "^2.2.3",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "vitepress": "1.0.0-beta.5"
  },
  "dependencies": {
    "@types/swagger-ui-express": "^4.1.3",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "convict": "^6.2.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.1",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.4.0",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "pug": "^3.0.2",
    "sharp": "^0.32.4",
    "slugify": "^1.6.6",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1",
    "validator": "^13.9.0",
